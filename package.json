  "version": "0.0.0",
  "description": "ShopBack Mobile Web Client",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors",
