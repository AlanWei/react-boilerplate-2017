{
  "sourceMaps": true,
  "presets": [
    ["es2015", { "modules": false }],
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-object-rest-spread",
    "dynamic-import-webpack",
    ["module-resolver", {
      "root": ["./src"]
    }]
  ],
  "env": {
    "production": {
      "sourceMaps": false,
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    },
    "test": {
      "presets": [
        "es2015"
      ],
      "plugins": [
        "dynamic-import-webpack"
      ]
    }
  }
}
